{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["plugin:react/recommended", "airbnb", "prettier"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react", "prettier", "eslint-plugin-prettier"],
    "rules": {
        "no-console": "off",
        "prettier/prettier": "error",
        "import/no-extraneous-dependencies": [
            "error",
            { "devDependencies": true }
        ]
    }
}
